package VegAndBilling ;

import java.util.Scanner;

import MilkDep.Milkproduct;

public class vegtablesItems extends Milkproduct{
	public class DAYone {
		public int Rice() {
			Scanner sc=new Scanner(System.in);
			int Rice_price=sc.nextInt();
			System.out.println("Rice price is :"+Rice_price);
			int Rice_Qty=sc.nextInt();
			System.out.println("Rice QTY is :"+Rice_Qty);
			int total_price=Rice_price*Rice_Qty;
			System.out.println("Total rice price :"+total_price);
			return total_price;
		}
		
	public static void main(String[] args) {
		vegtablesItems bi=new vegtablesItems();
		
		
	}		
	}
}
