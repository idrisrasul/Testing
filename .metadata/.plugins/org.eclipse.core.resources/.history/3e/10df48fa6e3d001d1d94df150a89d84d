package Newpro;

import java.util.Scanner;

public class DAYone {
	public int Rice() {
		Scanner sc=new Scanner(System.in);
		System.out.println("Rice price is :");
		int Rice_price=sc.nextInt();
		System.out.println("Rice QTY is :");
		int Rice_Qty=sc.nextInt();
		int total_price=Rice_price*Rice_Qty;
		return total_price;
	}

	public static void main(String[] args) {
		DAYone rc=new DAYone();
		System.out.println(rc.Rice());
	}
}
